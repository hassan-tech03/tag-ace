// Floating Back to Top Button
.floating-back-to-top {
  position: fixed;
  bottom: 30px;
  right: 30px;
  width: 50px;
  height: 50px;
  background: var(--purple-bg);
  color: white;
  border: none;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  cursor: pointer;
  box-shadow: 0 4px 20px rgba(139, 90, 124, 0.3);
  transition: all 0.3s ease;
  z-index: 1000;
  animation: fadeInUp 0.3s ease;
  
  &:hover {
    background: var(--dark-purple);
    transform: translateY(-3px);
    box-shadow: 0 8px 25px rgba(139, 90, 124, 0.4);
  }
  
  &:active {
    transform: translateY(-1px);
  }
  
  svg {
    width: 22px;
    height: 22px;
    stroke-width: 2.5;
  }
  
  @media (max-width: 768px) {
    bottom: 20px;
    right: 20px;
    width: 45px;
    height: 45px;
    
    svg {
      width: 20px;
      height: 20px;
    }
  }
  
  @media (max-width: 576px) {
    bottom: 15px;
    right: 15px;
    width: 40px;
    height: 40px;
    
    svg {
      width: 18px;
      height: 18px;
    }
  }
}

@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

