/* UNIVERSAL PRODUCT CARD STYLES */
.universal-product-card {
  background: transparent;
  border-radius: 15px;
  overflow: hidden;
  transition: all 0.3s ease;
  position: relative;
  height: fit-content;
  
  &:hover {
    transform: translateY(-10px);
    box-shadow: 0 20px 40px rgba(0,0,0,0.15);
    
    .product-image {
      .main-image {
        opacity: 0;
      }
      
      .hover-image {
        opacity: 1;
      }
      
      .product-actions {
        opacity: 1;
        visibility: visible;
        transform: translateX(0);
      }
    }
    
    .product-info {
      background: white;
      border-radius: 0 0 15px 15px;
    }
  }
  transition: all 0.3s ease;
  position: relative;
  height: fit-content;
  box-shadow: 0 5px 20px rgba(0,0,0,0.08);
  
  &:hover {
    transform: translateY(-10px);
    box-shadow: 0 20px 40px rgba(0,0,0,0.15);
    
    .product-card-image {
      .main-image {
        opacity: 0;
      }
      
      .hover-image {
        opacity: 1;
      }
      
      .product-card-actions {
        opacity: 1;
        visibility: visible;
        transform: translateX(0);
      }
    }
  }
  
  .product-card-link {
    text-decoration: none !important;
    color: inherit !important;
    display: block;
    
    &:hover,
    &:focus,
    &:active,
    &:visited {
      text-decoration: none !important;
      color: inherit !important;
    }
  }
  
  .product-card-image {
    position: relative;
    background: #f8f9fa;
    padding: 0;
    height: 280px;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
    border-radius: 15px 15px 0 0;
    
    @media (max-width: 576px) {
      height: 240px;
    }
    
    .product-badge {
      position: absolute;
      top: 15px;
      left: 15px;
      background: var(--purple-bg);
      color: white;
      padding: 6px 12px;
      border-radius: 20px;
      font-size: 11px;
      font-weight: 600;
      z-index: 3;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }
    
    .out-of-stock-badge {
      position: absolute;
      top: 15px;
      right: 15px;
      background: #dc3545;
      color: white;
      padding: 6px 12px;
      border-radius: 20px;
      font-size: 11px;
      font-weight: 600;
      z-index: 3;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }
    
    .image-container {
      position: relative;
      width: 100%;
      height: 100%;
      display: flex;
      align-items: center;
      justify-content: center;
    }
    
    .main-image {
      position: absolute !important;
      top: 0 !important;
      left: 0 !important;
      width: 100% !important;
      height: 100% !important;
      object-fit: cover !important;
      transition: opacity 0.3s ease !important;
      opacity: 1;
    }
    
    .hover-image {
      position: absolute !important;
      top: 0 !important;
      left: 0 !important;
      width: 100% !important;
      height: 100% !important;
      object-fit: cover !important;
      transition: opacity 0.3s ease !important;
      opacity: 0;
      z-index: 1;
    }
    
    .product-card-actions {
      position: absolute;
      top: 20px;
      right: 20px;
      display: flex;
      flex-direction: column;
      gap: 10px;
      opacity: 0;
      visibility: hidden;
      transform: translateX(20px);
      transition: all 0.4s ease;
      z-index: 10;
      
      .action-btn {
        width: 40px;
        height: 40px;
        background: white;
        border: none;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        cursor: pointer;
        box-shadow: 0 4px 15px rgba(0,0,0,0.15);
        transition: all 0.3s ease;
        color: #666;
        
        &:hover {
          background: var(--purple-bg);
          color: white;
          transform: scale(1.1);
          box-shadow: 0 6px 20px rgba(139, 90, 124, 0.3);
        }
        
        &:disabled {
          opacity: 0.5;
          cursor: not-allowed;
          
          &:hover {
            background: white;
            color: #666;
            transform: none;
          }
        }
        
        svg {
          width: 18px;
          height: 18px;
          stroke-width: 1.5;
        }
      }
    }
  }
  
  .product-card-info {
    padding: 20px;
    text-align: center;
    
    @media (max-width: 576px) {
      padding: 15px;
    }
    
    .product-rating {
      margin-bottom: 12px;
      
      .stars {
        display: flex;
        justify-content: center;
        gap: 2px;
        
        .star {
          color: #ffc107;
          font-size: 14px;
        }
      }
    }
    
    .product-name {
      font-size: 1.1rem;
      font-weight: 600;
      color: var(--primary-color);
      margin-bottom: 15px;
      font-family: 'Playfair Display', serif;
      text-decoration: none !important;
      line-height: 1.3;
      
      @media (max-width: 576px) {
        font-size: 1rem;
      }
      
      &:hover {
        text-decoration: none !important;
        color: inherit !important;
      }
    }
    
    .product-price {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      flex-wrap: wrap;
      
      .current-price {
        font-size: 1.2rem;
        font-weight: 700;
        color: var(--purple-bg);
      }
      
      .original-price {
        font-size: 1rem;
        color: #999;
        text-decoration: line-through;
      }
    }
  }
}

/* Responsive Grid Layouts */
.products-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 25px;
  
  @media (max-width: 1200px) {
    grid-template-columns: repeat(3, 1fr);
    gap: 20px;
  }
  
  @media (max-width: 768px) {
    grid-template-columns: repeat(2, 1fr);
    gap: 15px;
  }
  
  @media (max-width: 576px) {
    grid-template-columns: repeat(2, 1fr);
    gap: 12px;
  }
}

/* Swiper Specific Styles */
.swiper-slide .universal-product-card {
  width: 100%;
  max-width: 100%;
}