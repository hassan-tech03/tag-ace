      .tab-btn {
        background: none;
        border: none;
        font-size: 16px;
        font-weight: 500;
        color: #666;
        cursor: pointer;
        padding: 10px 0;
        position: relative;
        transition: color 0.3s ease;
        
        &.active {
          color: var(--purple-bg);
          
          &::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            right: 0;
            height: 2px;
            background: var(--purple-bg);
          }
        }
        
        &:hover {
          color: var(--purple-bg);
        }
      }
    }
    
    // Bootstrap Nav Tabs Styling
    .nav-tabs {
      border-bottom: 1px solid #dee2e6;
      margin-bottom: 40px;
      
      .nav-link {
        background: none;
        border: none;
        font-size: 16px;
        font-weight: 500;
        color: #666;
        cursor: pointer;
        padding: 15px 30px;
        position: relative;
        transition: all 0.3s ease;
        border-radius: 0;
        
        &.active {
          color: var(--purple-bg);
          background: none;
          border-bottom: 2px solid var(--purple-bg);
        }
        
        &:hover {
          color: var(--purple-bg);
          border-color: transparent;
        }
        
        @media (max-width: 576px) {
          padding: 12px 20px;
          font-size: 14px;
        }
      }
    }
  }
  
  .products-container {
    position: relative;
    
    .loading-overlay {
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background: rgba(255, 255, 255, 0.8);
      display: flex;
      align-items: center;
      justify-content: center;
      z-index: 10;
      border-radius: 15px;
      
      .loading-spinner {
        width: 40px;
        height: 40px;
        border: 3px solid #f3f3f3;
        border-top: 3px solid var(--purple-bg);
        border-radius: 50%;
        animation: spin 1s linear infinite;
      }
    }
  }
  
  .products-grid {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 30px;
    
    @media (max-width: 1200px) {
      grid-template-columns: repeat(3, 1fr);
      gap: 25px;
    }
    
    @media (max-width: 768px) {
      grid-template-columns: repeat(2, 1fr);
      gap: 20px;
    }
    
    @media (max-width: 576px) {
      grid-template-columns: repeat(2, 1fr);
      gap: 15px;
    }
    
    // Animation states
    &.changing {
      .product-item {
        opacity: 0;
        transform: translateY(20px);
      }
    }
  }
  
  // SwiperJS Styles
  .products-swiper {
    padding: 20px 0 60px;
    
    &.changing {
      .product-item {
        opacity: 0;
        transform: translateY(20px);
      }
    }
    
    // Hide default navigation since we're using custom
    .swiper-button-next,
    .swiper-button-prev {
      display: none;
    }
    
    .swiper-pagination {
      bottom: 10px;
      
      .swiper-pagination-bullet {
        background: #ddd;
        opacity: 1;
        width: 12px;
        height: 12px;
        margin: 0 6px;
        transition: all 0.3s ease;
        
        &.swiper-pagination-bullet-active {
          background: var(--purple-bg);
          transform: scale(1.2);
        }
        
        &:hover {
          background: var(--purple-bg);
          opacity: 0.7;
        }
      }
    }
  }
  
  .product-item {
    background: transparent;
    border-radius: 15px;
    overflow: hidden;
    transition: all 0.3s ease;
    position: relative;
    opacity: 1;
    transform: translateY(0);
    animation: fadeInUp 0.6s ease forwards;
    height: fit-content;
    
    &:hover {
      transform: translateY(-10px);
      box-shadow: 0 20px 40px rgba(0,0,0,0.15);
      
      .product-image {
        .main-image {
          opacity: 0;
        }
        
        .hover-image {
          opacity: 1;
        }
        
        .product-actions {
          opacity: 1;
          visibility: visible;
          transform: translateX(0);
        }
      }
      
      .product-info {
        background: white;
        border-radius: 0 0 15px 15px;
      }
    }
    
    .product-image {
      position: relative;
      background: transparent;
      padding: 0;
      height: 300px;
      display: flex;
      align-items: center;
      justify-content: center;
      overflow: hidden;
      border-radius: 15px 15px 0 0;
      
      @media (max-width: 576px) {
        height: 250px;
      }
      
      .sale-badge {
        position: absolute;
        top: 15px;
        left: 15px;
        background: var(--primary-color);
        color: white;
        padding: 5px 12px;
        border-radius: 20px;
        font-size: 12px;
        font-weight: 600;
        z-index: 2;
      }
      
      .main-image {
        position: absolute !important;
        top: 0 !important;
        left: 0 !important;
        width: 100% !important;
        height: 100% !important;
        object-fit: cover !important;
        transition: opacity 0.3s ease !important;
        opacity: 1;
      }
      
      .hover-image {
        position: absolute !important;
        top: 0 !important;
        left: 0 !important;
        width: 100% !important;
        height: 100% !important;
        object-fit: cover !important;
        transition: opacity 0.3s ease !important;
        opacity: 0;
        z-index: 1;
      }
      
      .product-actions {
        position: absolute;
        top: 20px;
        right: 20px;
        display: flex;
        flex-direction: column;
        gap: 12px;
        opacity: 0;
        visibility: hidden;
        transform: translateX(20px);
        transition: all 0.4s ease;
        z-index: 10;
        
        .action-btn {
          width: 45px;
          height: 45px;
          background: white;
          border: none;
          border-radius: 50%;
          display: flex;
          align-items: center;
          justify-content: center;
          cursor: pointer;
          box-shadow: 0 4px 20px rgba(0,0,0,0.15);
          transition: all 0.3s ease;
          color: #666;
          
          &:hover {
            background: var(--primary-color);
            color: white;
            transform: scale(1.15);
            box-shadow: 0 6px 25px rgba(44, 62, 80, 0.3);
          }
          
          svg {
            width: 20px;
            height: 20px;
            stroke-width: 1.5;
          }
        }
      }
    }
    
    .product-info {
      padding: 25px 20px;
      text-align: center;
      
      @media (max-width: 576px) {
        padding: 20px 15px;
      }
      
      .product-rating {
        margin-bottom: 10px;
        
        span {
          color: #ffc107;
          font-size: 14px;
        }
      }
      
      h3 {
        font-size: 1.1rem;
        font-weight: 600;
        color: var(--primary-color);
        margin-bottom: 15px;
        font-family: 'Playfair Display', serif;
        
        @media (max-width: 576px) {
          font-size: 1rem;
        }
      }
      
      .product-price {
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        
        .current-price {
          font-size: 1.2rem;
          font-weight: 700;
          color: var(--primary-color);
        }
        
        .original-price {
          font-size: 1rem;
          color: #999;
          text-decoration: line-through;
        }
      }
    }
  }
}

// Keyframe Animations for Tab Changes
@keyframes fadeInUp {
  0% {
    opacity: 0;
    transform: translateY(30px);
  }
  100% {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes fadeOut {
  0% {
    opacity: 1;
    transform: translateY(0);
  }
  100% {
    opacity: 0;
    transform: translateY(-20px);
  }
}

@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

