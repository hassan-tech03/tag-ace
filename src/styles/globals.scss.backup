        &.swiper-pagination-bullet-active {
          background: var(--purple-bg);
          border-color: rgba(139, 90, 124, 0.2);
          transform: scale(1.3);
          box-shadow: 0 2px 8px rgba(139, 90, 124, 0.3);
        }
        
        &:hover:not(.swiper-pagination-bullet-active) {
          background: #adb5bd;
          transform: scale(1.1);
        }
      }
    }
