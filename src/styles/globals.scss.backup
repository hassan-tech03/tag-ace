    // Custom Navigation Icons - Between Tabs and Cards
    .custom-navigation {
      display: flex;
      justify-content: flex-end;
      gap: 10px;
      margin-top: 15px;
      margin-bottom: -20px;
      
      @media (max-width: 768px) {
        justify-content: flex-end;
