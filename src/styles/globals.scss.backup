// Fix for image loading and flickering issues
.shop-page .product-item .product-image,
.category-shop-page .product-item .product-image {
  .main-image,
  .hover-image {
    // Ensure images maintain their dimensions during loading
    min-height: 300px;
    background: #f8f9fa;
    transition: opacity 0.3s ease;
    
    // Prevent flickering during initial load
    &:not(.loaded) {
      opacity: 0.7;
    }
    
    &.loaded {
      opacity: 1;
    }
    
    @media (max-width: 576px) {
      min-height: 250px;
    }
  }
  
  .hover-image {
    &.loaded {
      opacity: 0; // Still hidden until hover
    }
  }
  
  // Prevent layout shift during image loading
  &::before {
    content: '';
    display: block;
    width: 100%;
    height: 100%;
    background: #f8f9fa;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
  }
  
  img {
    position: relative;
    z-index: 1;
  }
}

// Prevent page refresh issues with useEffect
.shop-page,
.category-shop-page {
  // Ensure stable layout during hydration
  min-height: 100vh;
  
  .products-container {
    // Prevent layout shift during mobile detection
    min-height: 400px;
    
    .products-grid,
    .swiper-container {
      opacity: 1;
      transition: opacity 0.3s ease;
    }
  }
  
  // Smooth image loading animation
  .product-item {
    .product-image {
      .main-image.loaded,
      .hover-image.loaded {
        animation: fadeInImage 0.5s ease-in-out;
      }
    }
  }
}

@keyframes fadeInImage {
  from {
    opacity: 0;
    transform: scale(0.95);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}
