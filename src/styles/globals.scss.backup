  .popular-products-container {
    position: relative;
    margin-bottom: 50px;
    overflow: hidden; // Prevent overflow
    
    .popular-products-swiper {
      padding: 20px 0 70px;
      margin: 0 -15px; // Negative margin to compensate for button positioning
      
      .swiper-slide {
        height: auto;
        transition: transform 0.3s ease;
        padding: 0 15px; // Add padding to slides
        
        &:hover {
          transform: translateY(-5px);
        }
      }
      
      // Custom Navigation Buttons
      .swiper-nav-btn {
        position: absolute;
        top: 50%;
        transform: translateY(-50%);
        width: 55px;
        height: 55px;
        background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
        border: none;
        border-radius: 50%;
        display: flex;
        align-items: center;
        justify-content: center;
        cursor: pointer;
        box-shadow: 0 8px 25px rgba(0,0,0,0.12);
        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);
        color: var(--purple-bg);
        z-index: 10;
        
        &::before {
          content: '';
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
          background: linear-gradient(135deg, var(--purple-bg) 0%, var(--dark-purple) 100%);
          border-radius: 50%;
          opacity: 0;
          transition: opacity 0.4s ease;
          z-index: -1;
        }
        
        &:hover {
          color: white;
          transform: translateY(-50%) translateY(-5px) scale(1.05);
          box-shadow: 0 15px 35px rgba(139, 90, 124, 0.3);
          
          &::before {
            opacity: 1;
          }
        }
        
        &:active {
          transform: translateY(-50%) translateY(-2px) scale(1.02);
          box-shadow: 0 8px 20px rgba(44, 62, 80, 0.25);
        }
        
        svg {
          width: 22px;
          height: 22px;
          stroke-width: 2.5;
          transition: all 0.3s ease;
          position: relative;
          z-index: 1;
        }
        
        &:hover svg {
          transform: scale(1.15);
          filter: drop-shadow(0 2px 4px rgba(255,255,255,0.3));
        }
        
        &.swiper-button-disabled {
          opacity: 0.4;
          cursor: not-allowed;
          background: #f1f3f4;
          color: #9aa0a6;
          
          &:hover {
            transform: translateY(-50%);
            box-shadow: 0 8px 25px rgba(0,0,0,0.12);
            color: #9aa0a6;
            
            &::before {
              opacity: 0;
            }
            
            svg {
              transform: none;
              filter: none;
            }
          }
        }
        
        @media (max-width: 768px) {
          width: 50px;
          height: 50px;
          
          svg {
            width: 20px;
            height: 20px;
          }
        }
        
        @media (max-width: 576px) {
          width: 45px;
          height: 45px;
          
          svg {
            width: 18px;
            height: 18px;
          }
        }
      }
    }
    
    .popular-swiper-button-prev,
    .popular-swiper-button-next {
      position: absolute;
      top: 50%;
      transform: translateY(-50%);
      width: 50px;
      height: 50px;
      background: white;
      border: 2px solid #e9ecef;
      border-radius: 50%;
      display: flex;
      align-items: center;
      justify-content: center;
      cursor: pointer;
      box-shadow: 0 4px 15px rgba(0,0,0,0.1);
      transition: all 0.3s ease;
      color: var(--primary-color);
      z-index: 10;
      
      &:hover {
        background: var(--purple-bg);
        color: white;
        border-color: var(--purple-bg);
        transform: translateY(-50%) scale(1.1);
        box-shadow: 0 8px 25px rgba(139, 90, 124, 0.3);
      }
      
      &:active {
        transform: translateY(-50%) scale(0.95);
      }
      
      svg {
        width: 20px;
        height: 20px;
        stroke-width: 2.5;
      }
      
      &.swiper-button-disabled {
        opacity: 0.3;
        cursor: not-allowed;
        
        &:hover {
          background: white;
          color: var(--primary-color);
          border-color: #e9ecef;
          transform: translateY(-50%);
          box-shadow: 0 4px 15px rgba(0,0,0,0.1);
        }
      }
      
      @media (max-width: 768px) {
        width: 45px;
        height: 45px;
        
        svg {
          width: 18px;
          height: 18px;
        }
      }
      
      @media (max-width: 576px) {
        width: 40px;
        height: 40px;
        
        svg {
          width: 16px;
          height: 16px;
        }
      }
    }
    
    // Custom Pagination
    .popular-swiper-pagination {
      bottom: 15px;
      text-align: center;
      
      .swiper-pagination-bullet {
        background: #dee2e6;
