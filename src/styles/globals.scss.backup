        justify-content: center;
        
        img {
          width: 100% !important;
          height: 100% !important;
          object-fit: cover !important;
          transition: transform 0.3s ease;
        }
        
        &:hover img {
          transform: scale(1.05);
        }
        
        .zoom-icon {
          position: absolute;
          top: 20px;
          right: 20px;
          width: 50px;
          height: 50px;
          background: rgba(255, 255, 255, 0.9);
          border-radius: 50%;
          display: flex;
          align-items: center;
          justify-content: center;
          color: var(--purple-bg);
          text-decoration: none;
          opacity: 0;
          visibility: hidden;
          transition: all 0.3s ease;
          backdrop-filter: blur(10px);
          
          &:hover {
            background: var(--purple-bg);
            color: white;
            transform: scale(1.1);
          }
        }
        
        &:hover .zoom-icon {
          opacity: 1;
          visibility: visible;
        }
      }
    }
    
    .thumbnail-images {
      display: flex;
      gap: 15px;
      flex-wrap: wrap;
      
      .thumbnail {
        width: 80px;
        height: 80px;
        border-radius: 10px;
        overflow: hidden;
        cursor: pointer;
        border: 2px solid transparent;
        transition: all 0.3s ease;
        
        &.active {
          border-color: var(--purple-bg);
        }
        
        &:hover {
          border-color: var(--purple-bg);
          opacity: 0.8;
        }
        
        img {
          width: 100% !important;
          height: 100% !important;
          object-fit: cover !important;
        }
      }
      
      @media (max-width: 576px) {
        .thumbnail {
          width: 60px;
          height: 60px;
        }
      }
    }
  }
  
  // Product Info
  .product-info {
    padding-left: 40px;
    
    @media (max-width: 991px) {
      padding-left: 0;
      margin-top: 40px;
    }
    
    .product-category {
      margin-bottom: 10px;
      
      a {
        color: var(--purple-bg);
        text-decoration: none;
        font-size: 14px;
        font-weight: 500;
        text-transform: uppercase;
        letter-spacing: 1px;
        
        &:hover {
          text-decoration: underline;
        }
      }
    }
    
    .product-title {
      font-size: 2.5rem;
      font-weight: 300;
      color: var(--primary-color);
      margin-bottom: 20px;
      font-family: 'Playfair Display', serif;
      
      @media (max-width: 768px) {
        font-size: 2rem;
      }
    }
    
    .product-rating {
      display: flex;
      align-items: center;
      gap: 10px;
      margin-bottom: 20px;
      
      .stars {
        display: flex;
        gap: 2px;
        
        .star {
          color: #ddd;
          font-size: 18px;
          
          &.filled {
            color: #ffc107;
          }
        }
      }
      
      .rating-text {
        color: #666;
        font-size: 14px;
      }
    }
    
    .product-price {
      display: flex;
      align-items: center;
      gap: 15px;
      margin-bottom: 25px;
      
      .current-price {
        font-size: 2rem;
        font-weight: 700;
        color: var(--purple-bg);
      }
      
      .original-price {
        font-size: 1.2rem;
        color: #999;
        text-decoration: line-through;
      }
      
      .discount {
        background: #e74c3c;
        color: white;
        padding: 4px 8px;
        border-radius: 4px;
        font-size: 12px;
        font-weight: 600;
      }
    }
    
    .product-description {
      margin-bottom: 30px;
      
      p {
        color: #666;
        line-height: 1.6;
        font-size: 16px;
      }
    }
    
    .product-actions {
      margin-bottom: 30px;
      
      .quantity-selector {
        display: flex;
        align-items: center;
        gap: 15px;
        margin-bottom: 25px;
        
        label {
          font-weight: 500;
          color: var(--primary-color);
        }
        
        .quantity-controls {
          display: flex;
          align-items: center;
          border: 2px solid #e9ecef;
          border-radius: 8px;
          overflow: hidden;
          
          .qty-btn {
            background: none;
            border: none;
            width: 40px;
            height: 40px;
            display: flex;
            align-items: center;
            justify-content: center;
            cursor: pointer;
            font-size: 18px;
            font-weight: 600;
            color: var(--purple-bg);
            transition: all 0.3s ease;
            
            &:hover {
              background: var(--purple-bg);
              color: white;
            }
          }
          
          .qty-display {
            min-width: 50px;
            text-align: center;
            font-weight: 600;
            padding: 0 10px;
            border-left: 1px solid #e9ecef;
            border-right: 1px solid #e9ecef;
          }
        }
      }
      
      .action-buttons {
        display: flex;
        gap: 15px;
        margin-bottom: 20px;
        
        @media (max-width: 576px) {
          flex-direction: column;
        }
        
        .btn {
          padding: 15px 30px;
          font-weight: 600;
          font-size: 14px;
          letter-spacing: 1px;
          text-transform: uppercase;
          border-radius: 8px;
          transition: all 0.3s ease;
          border: none;
          cursor: pointer;
          
          &.add-to-cart-btn {
            background: var(--purple-bg);
            color: white;
            flex: 1;
            
            &:hover {
              background: var(--dark-purple);
              transform: translateY(-2px);
            }
          }
          
          &.buy-now-btn {
            background: var(--primary-color);
            color: white;
            flex: 1;
            
            &:hover {
              background: #1a252f;
              transform: translateY(-2px);
            }
          }
        }
      }
      
      .wishlist-compare {
        .wishlist-btn {
          background: none;
          border: none;
          color: #666;
          display: flex;
          align-items: center;
          gap: 8px;
          cursor: pointer;
          font-size: 14px;
          transition: color 0.3s ease;
          
          &:hover {
            color: var(--purple-bg);
          }
          
          svg {
            stroke-width: 1.5;
          }
        }
      }
    }
    
    .product-features {
      border-top: 1px solid #e9ecef;
      padding-top: 25px;
      
      .feature {
        display: flex;
        align-items: center;
        gap: 12px;
        margin-bottom: 15px;
        color: #666;
        font-size: 14px;
        
        svg {
          color: var(--purple-bg);
          flex-shrink: 0;
        }
        
        &:last-child {
          margin-bottom: 0;
        }
      }
    }
  }
}

